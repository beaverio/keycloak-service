{
  "realm": "production",
  "enabled": true,
  "displayName": "Production",
  "displayNameHtml": "<div class=\"kc-logo-text\"><span>Production</span></div>",
  "accessTokenLifespan": 3600,
  "refreshTokenMaxReuse": 0,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionMaxLifespan": 5184000,
  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "sslRequired": "all",
  "registrationAllowed": false,
  "registrationEmailAsUsername": true,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "clients": [
    {
      "clientId": "auth-gateway",
      "name": "auth-gateway",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "REPLACE_WITH_PRODUCTION_SECRET",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "publicClient": false,
      "frontchannelLogout": false,
      "protocol": "openid-connect",
      "rootUrl": "https://app.beaver.com",
      "adminUrl": "https://app.beaver.com",
      "baseUrl": "/",
      "redirectUris": [
        "https://app.beaver.com/*"
      ],
      "webOrigins": [
        "https://app.beaver.com"
      ],
      "bearerOnly": false,
      "consentRequired": false,
      "defaultClientScopes": [
        "web-origins",
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Standard user role",
        "composite": false,
        "clientRole": false,
        "containerId": "production"
      },
      {
        "name": "admin",
        "description": "Administrator role",
        "composite": false,
        "clientRole": false,
        "containerId": "production"
      }
    ]
  }
}
