{
  "realm": "non-production",
  "enabled": true,
  "clients": [
    {
      "clientId": "auth-gateway",
      "protocol": "openid-connect",
      "publicClient": false,
      "clientAuthenticatorType": "client-secret",
      "standardFlowEnabled": true,
      "redirectUris": [
        "http://localhost:8000/login/oauth2/code/*"
      ],
      "webOrigins": [
        "http://localhost:8000"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user"
      },
      {
        "name": "admin"
      }
    ]
  },
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "test@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "password123",
          "temporary": false
        }
      ],
      "realmRoles": ["user"]
    },
    {
      "username": "testadmin",
      "enabled": true,
      "email": "admin@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "realmRoles": ["admin", "user"]
    }
  ]
}
